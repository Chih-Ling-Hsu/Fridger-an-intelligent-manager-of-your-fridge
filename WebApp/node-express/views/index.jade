extends layout

block content
  div(id="main")
  div(id="myModal" class="modal fade" role="dialog")
    div(class="modal-dialog")
      div(class="modal-content")
        div(class="modal-header")
          button(type="button" class="close" data-dismiss="modal") &times;
          h4(class="modal-title")        
        div(class="modal-body" style="text-align: center; padding-left:50px; padding-right:50px;")        
        div(class="modal-footer" style='margin:15px;, padding:0px; text-align: center; word-wrap: break-word;word-break: break-all;')  
  script(type='text/javascript' src='/javascripts/awsConfig.js')
  script(type='text/javascript' src='/javascripts/util.js')
  script(type='text/javascript' src='/javascripts/recipe.js')
  script(type='text/javascript' src='/javascripts/index.js')
